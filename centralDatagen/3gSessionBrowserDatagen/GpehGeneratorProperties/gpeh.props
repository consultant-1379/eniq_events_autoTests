###########################################################
#  GPEH 
###########################################################
[gpeh]

[basic]

# chose which encoder to encode GPEH files
# either W13A_CN1 or W11B_CF2
encoder_version=W13A_CN1

#
# Start and end time range of files in the master repository
# Time range should be within 24 hour
#
lookup_timerange_start = 201205161600
lookup_timerange_end = 201205171600

# gpeh master file repository
input_directory = /ossrc/GPEH_MASTER_REPOSITORY

# number of rncs to produce
number_of_rncs = 20

# rop time period
rop_time_period = 15

mz_property_file_directory = /opt/ericsson/3g-session-browser-datagen/etc/
mz_property_filename = configurationGPEH.properties

mzsh            = /eniq/home/dcuser/mz2/mz/bin/launchmzsh.sh 
mz_account      = mzadmin/central1
workgroup_name  = GPEH_3G_SESSION.WG01_GPEH_Enrichment
workflow_name_prefix = GPEH_3G_SESSION.WF01_GPEH_Enrichment.workflow_

