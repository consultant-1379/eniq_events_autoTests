###########################################################
#  TCP PARTIAL 
###########################################################
[tcp partial]

[basic]

# if not specified, default to False
enable_compression = True 

#
# Start and end time range of files in the master repository
# Time range should be within 24 hour
#
lookup_timerange_start = 201205161600
lookup_timerange_end   = 201205171600

# tcpta partial master file repository
input_directory  = /ossrc/TCPTA_PARTIAL_MASTER_REPOSITORY

rop_time_period = 5

# rop time period
number_of_probes = 1

#
# Number of RNC traffics that are captured by all PCP devices
#
number_of_rncs = 1

#
# Output directories where all enriched files are stored
#
[output_directories]
output_directory1=/ossrc/UP/PCP1/staple/3G/tcpta_partial
#output_directory2=/home/epstvxj/Development/Temp/test/UP/PCP2/staple/3G/tcpta_partial
