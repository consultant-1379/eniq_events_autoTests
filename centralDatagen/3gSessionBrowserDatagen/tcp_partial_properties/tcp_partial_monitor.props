###########################################################
#  Used by the Tcp Partial Generator, 
#  This configuration is used by scripts to copy
#  regenerated data into MZ and CEP input directory
###########################################################
[tcp_partial_monitor]

[basic]
rop_time_period = 5

# 
# This is only used for testing purpose, 
# should be set to False by default
#
ignore_session_time = False

number_of_rncs = 1

#
# directories where enriched files are stored by the generator
#
[generated_file_locations]               
generated_file_location1 = /ossrc/UP/PCP1/staple/3G/tcpta_partial
#generated_file_location2 = /ossrc/UP/PCP2/staple/3G/tcpta_partial
#generated_file_location3 = /ossrc/UP/PCP3/staple/3G/tcpta_partial
#generated_file_location4 = /ossrc/UP/PCP4/staple/3G/tcpta_partial

#
# Each generated_file_location<N> section contains a set of destination directories.
# Those destination directories are input directories of CEP and MZ
#
[generated_file_location1]
cep_copy_directory1=/ossrc/data/pmMediation/eventData/PCPEvents_CEP/PCP1/staple/3G/tcpta_partial

#[generated_file_location2]
#cep_copy_directory2=/ossrc/data/pmMediation/eventData/PCPEvents_CEP/PCP2/staple/3G/tcpta_partial

#[generated_file_location3]
#cep_copy_directory3=/ossrc/data/pmMediation/eventData/PCPEvents_CEP/PCP3/staple/3G/tcpta_partial

#[generated_file_location4]
#cep_copy_directory4=/ossrc/data/pmMediation/eventData/PCPEvents_CEP/PCP4/staple/3G/tcpta_partial

#
# This section contains name prefixes that will be appended to each generated file before
# copying them to the CEP and MZ input directories
#
[name prefixes]

#
# This section defines whether to move generated files to the destination folder.
# If False, then copy is used instead
#
[move]
cep_copy_directory1 = True 
#cep_copy_directory2 = True
#cep_copy_directory3 = True
#cep_copy_directory4 = True

