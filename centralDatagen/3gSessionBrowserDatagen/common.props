###########################################################
#  COMMON TO ALL TYPES OF DATA (GPEH, SGEH, USER PLANE) 
###########################################################
[common]

#
# To disable/enable data generation for each type of data
#

# to enable tcp-partial generator
enable_tcp_partial                      = True 
# tcp partial copy script
enable_tcp_partial_monitor              = True 
enable_tcp_partial_validator_monitor    = False

# to enable classification generator
enable_classification                   = True 
# classification copy script
enable_classification_monitor           = True 
enable_classification_validator_monitor = False

# to enable sgeh generator
enable_sgeh                             = True 
# sgeh copy script
enable_mz_sgeh_monitor                  = True 
enable_sgeh_validator_monitor           = False

# to enable gpeh generator
enable_gpeh                             = True 
# gpeh copy script
enable_mz_gpeh_monitor                  = True 
enable_gpeh_validator_monitor           = False


enrich_imsi      = True
enrich_datetime  = True
clean            = True
realtime         = True

logger_external_config = /opt/ericsson/3g-session-browser-datagen/etc/datagen_logging
