<assembly
	xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.0 http://maven.apache.org/xsd/assembly-1.1.0.xsd">
	<id></id>
	<baseDirectory>/</baseDirectory>
	<formats>
		<format>zip</format>
	</formats>
	<fileSets>
		<fileSet>
			<directory>${project.basedir}/automation</directory>
			<outputDirectory>automation</outputDirectory>
			<fileMode>0755</fileMode>
		</fileSet>
		<fileSet>
			<directory>${project.basedir}/eniq-events-ltees/target</directory>
			<outputDirectory>automation/test_automation_ltees</outputDirectory>
			<includes>
				<include>lteesAutomation.jar</include>
				<include>lteesAutomation_lib/*</include>
			</includes>
			<fileMode>0755</fileMode>
		</fileSet>
		<fileSet>
			<directory>${project.basedir}/eniq-events-ltees</directory>
			<outputDirectory>automation/test_automation_ltees</outputDirectory>
			<includes>
				<include>app.properties</include>
			</includes>
			<fileMode>0755</fileMode>
		</fileSet>
		
		<fileSet>
			<directory>${project.basedir}/ee-notification-service-test-automation/src/main/resources</directory>
			<outputDirectory>automation/test_automation_kpi_notification/kpi_resources</outputDirectory>
			<includes>
				<include>*.*</include>
			</includes>
			<fileMode>0755</fileMode>
		</fileSet>
		<fileSet>
			<directory>${project.basedir}/ee-notification-service-test-automation/target</directory>
			<outputDirectory>automation/test_automation_kpi_notification</outputDirectory>
			<includes>
				<include>kpiNotificationAutomation.jar</include>
				<include>kpiNotificationAutomation_lib/*</include>
			</includes>
			<fileMode>0755</fileMode>
		</fileSet>
		
		<fileSet>
			<directory>${project.basedir}/eniq-events-selenium/target</directory>
			<outputDirectory>selenium/selenium-grid-1.0.8/test-cases</outputDirectory>
			<includes>
				<include>selenium_events_tests.jar</include>
				<include>selenium_events_tests_lib/*</include>
			</includes>
			<fileMode>0755</fileMode>
		</fileSet>
		<fileSet>
			<directory>${project.basedir}/eniq-events-selenium/target/classes</directory>
			<outputDirectory>selenium/selenium-grid-1.0.8/test-cases</outputDirectory>
			<includes>
				<include>resources/*</include>
			</includes>
			<fileMode>0755</fileMode>
		</fileSet>
		<fileSet>
			<directory>${project.basedir}/eniq-events-selenium/target/test-classes/</directory>
			<outputDirectory>automation/selenium_grid_files</outputDirectory>
			<fileMode>0755</fileMode>
		</fileSet>
		<fileSet>
			<directory>${project.basedir}/selenium</directory>
			<outputDirectory>selenium</outputDirectory>
			<fileMode>0755</fileMode>
		</fileSet>
	</fileSets>
</assembly>